<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/list_fm_shopcart_img"
        android:layout_toRightOf="@+id/list_fm_shopcart_cb"
        android:layout_margin="@dimen/dp_10"
        android:src="@mipmap/icon_home_rel"
        android:layout_width="@dimen/dp_104"
        android:layout_height="@dimen/dp_87" />
    <TextView
        android:layout_alignTop="@+id/list_fm_shopcart_img"
        android:layout_toRightOf="@+id/list_fm_shopcart_img"
        android:text="商品名称商品名称商品名称商品名称商品名称商品名称......"
        android:textColor="#ff040000"
        android:textSize="14sp"
        android:lines="2"
        android:ellipsize="end"
        android:id="@+id/list_fm_shopcart_tv_content"
        android:layout_width="@dimen/dp_192"
        android:layout_height="@dimen/dp_44" />

    <TextView
        android:layout_below="@+id/list_fm_shopcart_tv_content"
        android:id="@+id/ist_fm_shopcart_tv_nature"
        android:layout_toRightOf="@+id/list_fm_shopcart_img"
        android:layout_marginTop="@dimen/dp_2"
        android:layout_width="match_parent"
        android:layout_height="16dp"
        android:text="30ml /绿色"
        android:textColor="#ff949397"
        android:textSize="12sp"
        />
    <TextView
        android:id="@+id/myorder_item_pursue"
        android:layout_toRightOf="@+id/list_fm_shopcart_img"
        android:layout_alignBottom="@+id/list_fm_shopcart_img"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="¥ 23.00"
        android:textColor="#ffefb134"
        android:textSize="14sp"
        />

     <TextView
         android:layout_marginRight="@dimen/dp_10"
         android:text="1"
         android:textSize="@dimen/sp_16"
         android:textColor="@color/text_black"
         android:layout_alignBottom="@+id/myorder_item_pursue"
         android:layout_alignTop="@+id/myorder_item_pursue"
         android:layout_alignParentRight="true"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content" />

     <TextView
         android:id="@+id/myorder_item_detail_fgx"
         android:layout_marginTop="@dimen/dp_21"
         android:layout_below="@+id/list_fm_shopcart_img"
         android:background="#ffe6e6e6"
         android:layout_width="match_parent"
         android:layout_height="0.5dp" />

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_12"
        android:visibility="visible"
        android:layout_below="@+id/myorder_item_detail_fgx"
        android:id="@+id/myorder_item_bottom_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_10"
            android:id="@+id/item_shopping_tv_goods"
            android:text="共2件商品,合计180元"
            android:textSize="@dimen/sp_16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:id="@+id/item_shopping_ll"
            android:layout_marginBottom="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_below="@+id/item_shopping_tv_goods"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <Button
                android:focusable="false"
                android:layout_marginRight="@dimen/dp_9"
                android:layout_marginLeft="@dimen/dp_9"
                android:background="@drawable/btn_enroll"
                android:text="取消订单"
                android:layout_weight="1"
                android:textSize="14sp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_36" />
            <Button
                android:focusable="false"
                android:layout_marginRight="@dimen/dp_9"
                android:layout_marginLeft="@dimen/dp_9"
                android:background="@drawable/btn_yellow"
                android:text="立即付款"
                android:textSize="14sp"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_36" />
        </LinearLayout>
        <View
            android:layout_below="@+id/item_shopping_ll"
            android:background="#ededed"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"/>
    </RelativeLayout>
</RelativeLayout>
